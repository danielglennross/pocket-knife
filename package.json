{
  "name": "pocket-knife",
  "scripts": {
    "clean": "cross-env git clean -dfqX -- ./node_modules **/{dist,node_modules}/",
    "compile": "cross-env tsc --build tsconfig.json",
    "compile:clean": "cross-env tsc --build tsconfig.json --clean",
    "watch": "cross-env tsc --build tsconfig.json --watch",
    "lint": "cross-env prettier-check \"**/*.{js,ts}\"",
    "lint-fix": "cross-env prettier \"**/*.{js,ts}\" --write",
    "commit": "git-cz"
  },
  "engines": {
    "node": ">=11.10.0",
    "npm": ">=6.7.0"
  },
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged"
    }
  },
  "lint-staged": {
    "*.{ts,js}": [
      "prettier --write",
      "git add"
    ]
  },
  "dependencies": {
    "@hapi/hoek": "^6.2.1",
    "@hapi/wreck": "^15.0.1",
    "bull": "^3.7.0",
    "http-proxy-agent": "^2.1.0",
    "ioredis": "^4.9.0",
    "json-bigint": "^0.3.0",
    "jsonwebtoken": "^8.5.1",
    "pem": "^1.14.2",
    "ramda": "^0.26.1",
    "redlock": "^3.1.2",
    "uuid": "^3.3.2",
    "word-wrap": "^1.2.3"
  },
  "devDependencies": {
    "@types/bull": "^3.5.11",
    "@types/hapi__hoek": "^6.2.0",
    "@types/hapi__wreck": "^15.0.0",
    "@types/ioredis": "^4.0.10",
    "@types/jsonwebtoken": "^8.3.2",
    "@types/node": "^11.11.6",
    "@types/pem": "^1.9.5",
    "@types/ramda": "^0.26.5",
    "@types/redlock": "^3.0.2",
    "@types/reflect-metadata": "^0.1.0",
    "@types/uuid": "^3.4.4",
    "@types/word-wrap": "^1.2.0",
    "commitizen": "^3.1.1",
    "cross-env": "^5.2.0",
    "cz-conventional-changelog": "^2.1.0",
    "husky": "^1.3.1",
    "lint-staged": "8.1.5",
    "prettier": "^1.16.4",
    "prettier-check": "2.0.0",
    "reflect-metadata": "^0.1.13",
    "semantic-release": "^15.13.16",
    "typescript": "^3.3.4000"
  },
  "optionalDependencies": {
    "ts-node": "^8.0.3"
  },
  "config": {
    "commitizen": {
      "path": "./node_modules/cz-conventional-changelog"
    }
  }
}
